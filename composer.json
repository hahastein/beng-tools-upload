{
  "name": "bengbengsoft/beng-tools",
  "description": "bengbeng tools for Yii2 ",
  "keywords": ["yii2", "tools", "bengbeng"],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "hahastein",
      "email": "146119@qq.com"
    }
  ],
  "require": {
    "yiisoft/yii2": "~2.0.7",
    "yiisoft/yii2-imagine": "^2.0@dev",
    "upyun/sdk": "dev-master",
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "bengbeng\\framework\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    },
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  }
}
